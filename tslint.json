{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "no-switch-case-fall-through": false,
    "no-console": [
      true
    ],
    "object-literal-shorthand": false,
    "prefer-for-of": true,
    "prefer-const": true,
    "no-unused-variable": [
      true
    ],
    "no-undef": [true]
  }
}
